# Your Application Secret (Must match what you send in the POST request)
MY_SECRET=your_secure_secret_value

# LLM Provider Configuration (openai, anthropic, or openrouter)
LLM_PROVIDER=openai

# API Keys (Uncomment and fill the one you need)
OPENAI_API_KEY=sk-proj-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# OPENROUTER_API_KEY=sk-or-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Model Selection
LLM_MODEL=gpt-3.5-turbo
